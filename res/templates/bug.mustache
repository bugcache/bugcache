<!DOCTYPE html>
<html>
	<head>
		<title>Bugcache</title>
	</head>
	<body>
		<a href="/recent">Recent bugs</a>
		<a href="/{{data.id}}/edit">Edit this bug</a>
		<h2>#{{data.id}} &mdash; {{data.title}}</h2>
		<div>Submitter: {{data.submittername}}</div>
		{{#data.attributes}}
			<div>{{name}}: <ul>{{#value}}<li>{{value}}</li>{{/value}}</ul></div>
		{{/data.attributes}}
		{{data.data}}
	</body>
</html>